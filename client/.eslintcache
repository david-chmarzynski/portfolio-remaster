 let menuLayer = useRef(null);\n  let reveal1 = useRef(null);\n  let reveal2 = useRef(null);\n  let info = useRef(null);\n  let line1 = useRef(null);\n  let line2 = useRef(null);\n  let line3 = useRef(null);\n\n  // USE EFFECT\n  useEffect(() => {\n    // MENU IS OPENED, WANT TO CLOSE IT\n    if (state.initial === false) {\n      staggerRevealClose(reveal2, reveal1);\n      gsap.to(menuLayer, { duration: 1, css: { display: \"none\" } });\n    } else if (state.initial === true) {\n      // MENU IS CLOSED, WANT TO OPEN IT\n      gsap.to(menuLayer, { duration: 0, css: { display: \"block\" } });\n      gsap.to([reveal1, reveal2], {\n        duration: 0,\n        opacity: 1,\n        height: \"100%\"\n      });\n      staggerReveal(reveal1, reveal2);\n      fadeInUp(info);\n      staggerText(line1, line2, line3);\n    }\n  }, [state]);\n\n  return (\n    <StyledHome ref={el => (menuLayer = el)}>\n      <StyledSecondaryBackground ref={el => (reveal1 = el)}>\n\n      </StyledSecondaryBackground>\n      <StyledMenuLayer ref={el => (reveal2 = el)}>\n        <StyledInfoBackground ref={el => (infoBackground = el)}>\n\n        </StyledInfoBackground>\n        <StyledContainer>\n          <StyledWrapper>\n            <StyledMenuLinks>\n              <StyledNav>\n                <StyledUl>\n                  <StyledLi>\n                    <Link\n                      onMouseEnter={e => handleHover(e)}\n                      onMouseOut={e => handleHoverExit(e)}\n                      ref={el => (line1 = el)}\n                      to=\"/technos\"\n                    >\n                      Technos.\n                    </Link>\n                  </StyledLi>\n                  <StyledLi>\n                    <Link\n                      onMouseEnter={e => handleHover(e)}\n                      onMouseOut={e => handleHoverExit(e)}\n                      ref={el => (line2 = el)}\n                      to=\"/roadmap\"\n                    >\n                      Roadmap.\n                    </Link>\n                  </StyledLi>\n                  <StyledLi>\n                    <Link\n                      onMouseEnter={e => handleHover(e)}\n                      onMouseOut={e => handleHoverExit(e)}\n                      ref={el => (line3 = el)}\n                      to=\"/contact\"\n                    >\n                      Contact.\n                    </Link>\n                  </StyledLi>\n                </StyledUl>\n              </StyledNav>\n              <StyledInfo ref={el => (info = el)}>\n                <h3>A propos.</h3>\n                <p>\n                  Développeur web Javascript, spécialisé dans\n                  l'utilisation du framework ReactJS, je possède\n                  également des compétences en NodeJS.\n                  Retrouvez mes différents projets ci-dessous.\n                </p>\n              </StyledInfo>\n              <StyledProject>\n                Infos:\n                {infos.map(el => (\n                  <span\n                    key={el.name}\n                    onMouseEnter={() => handleInfo(el.image, infoBackground)}\n                    onMouseOut={() => handleInfoReturn(infoBackground)}\n                  >\n                    {el.name}\n                  </span>\n                ))}\n              </StyledProject>\n              <StyledSocials>\n\n              </StyledSocials>\n            </StyledMenuLinks>\n          </StyledWrapper>\n        </StyledContainer>\n      </StyledMenuLayer>\n    </StyledHome>\n  );\n};\n\nexport default Home;\n","/home/david/Projets/portfolio-remaster/client/src/components/Home/Home.styled.js",[],"/home/david/Projets/portfolio-remaster/client/src/components/Home/Home.animation.js",[],"/home/david/Projets/portfolio-remaster/client/src/components/Header/Header.js",[],"/home/david/Projets/portfolio-remaster/client/src/index.js",[],"/home/david/Projets/portfolio-remaster/client/src/store/index.js",[],"/home/david/Projets/portfolio-remaster/clie